<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<!-- Privacy Nutrition Label: Data collected and its purposes -->
	<key>NSPrivacyCollectedDataTypes</key>
	<array>
		<!-- Contact Info (name, phone, email, address) imported from Contacts -->
		<dict>
			<key>NSPrivacyCollectedDataType</key>
			<string>NSPrivacyCollectedDataTypeContactInfo</string>
			<key>NSPrivacyCollectedDataTypeLinked</key>
			<false/>
			<key>NSPrivacyCollectedDataTypeTracking</key>
			<false/>
			<key>NSPrivacyCollectedDataTypePurposes</key>
			<array>
				<string>NSPrivacyCollectedDataTypePurposeAppFunctionality</string>
			</array>
		</dict>
		<!-- Photos (avatar images for birthday persons) -->
		<dict>
			<key>NSPrivacyCollectedDataType</key>
			<string>NSPrivacyCollectedDataTypePhotosorVideos</string>
			<key>NSPrivacyCollectedDataTypeLinked</key>
			<false/>
			<key>NSPrivacyCollectedDataTypeTracking</key>
			<false/>
			<key>NSPrivacyCollectedDataTypePurposes</key>
			<array>
				<string>NSPrivacyCollectedDataTypePurposeAppFunctionality</string>
			</array>
		</dict>
	</array>

	<!-- Privacy Tracking: App does NOT track users across apps -->
	<key>NSPrivacyTracking</key>
	<false/>

	<!-- Required Reason APIs used by the app or its dependencies -->
	<key>NSPrivacyAccessedAPITypes</key>
	<array>
		<!-- UserDefaults (SharedPreferences uses NSUserDefaults) -->
		<dict>
			<key>NSPrivacyAccessedAPIType</key>
			<string>NSPrivacyAccessedAPICategoryUserDefaults</string>
			<key>NSPrivacyAccessedAPITypeReasons</key>
			<array>
				<!-- CA92.1: Access info from same app -->
				<string>CA92.1</string>
			</array>
		</dict>
		<!-- File timestamp APIs (SQLite/sqflite uses file stat) -->
		<dict>
			<key>NSPrivacyAccessedAPIType</key>
			<string>NSPrivacyAccessedAPICategoryFileTimestamp</string>
			<key>NSPrivacyAccessedAPITypeReasons</key>
			<array>
				<!-- C617.1: Access file timestamps inside app container -->
				<string>C617.1</string>
			</array>
		</dict>
		<!-- Disk space APIs (may be used by dependencies) -->
		<dict>
			<key>NSPrivacyAccessedAPIType</key>
			<string>NSPrivacyAccessedAPICategoryDiskSpace</string>
			<key>NSPrivacyAccessedAPITypeReasons</key>
			<array>
				<!-- E174.1: Check available disk space -->
				<string>E174.1</string>
			</array>
		</dict>
		<!-- System boot time APIs (may be used by dependencies for timing) -->
		<dict>
			<key>NSPrivacyAccessedAPIType</key>
			<string>NSPrivacyAccessedAPICategorySystemBootTime</string>
			<key>NSPrivacyAccessedAPITypeReasons</key>
			<array>
				<!-- 35F9.1: Measure elapsed time -->
				<string>35F9.1</string>
			</array>
		</dict>
	</array>
</dict>
</plist>
